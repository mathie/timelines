<%= render 'shared/errors', model: timeline %>

<%= bootstrap_form_for timeline, html: { multipart: true } do |f| %>
  <%= f.text_field :title, autofocus: true %>
  <%= f.text_area :body %>

  <%= f.text_field :cover_image_caption %>
  <%= f.text_field :cover_image_credit %>
  <%= f.file_field :cover_image %>

  <%= f.submit %>
<% end %>